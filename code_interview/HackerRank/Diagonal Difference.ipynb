    "def diagonalDifference(arr):\n",
    "    diag1 = []\n",
    "    diag2 = []\n",
    "    for i in range(len(arr)):\n",
    "        diag1.append(arr[i][i])\n",
    "        diag2.append(arr[i][len(arr)-1-i])\n",
    "    return abs(sum(diag1) - sum(diag2))\n",
    "        \n",
    "    \n"
