    "def birthdayCakeCandles(candles):\n",
    "    maximum = max(candles)\n",
    "    output = 0\n",
    "    for candle in candles:\n",
    "        if candle == maximum:\n",
    "            output += 1\n",
    "            \n",
    "    return output"
