   "source": [
    "from collections import Counter\n",
    "def makeAnagram(a, b):\n",
    "    d = Counter(a)\n",
    "    d.subtract(Counter(b))\n",
    "    output = sum([abs(x) for x in d.values()])\n",
    "    return output"
   ]
  },
